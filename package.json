{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "2.0.1",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "bootstrap": "4.4.1",
    "font-awesome": "4.7.0",
    "gatsby": "2.18.4",
    "gatsby-image": "2.2.34",
    "gatsby-link": "2.2.25",
    "gatsby-plugin-google-analytics": "2.1.29",
    "gatsby-plugin-manifest": "2.2.30",
    "gatsby-plugin-netlify": "2.1.27",
    "gatsby-plugin-offline": "3.0.24",
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-sass": "2.1.24",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.3.4",
    "gatsby-plugin-sitemap": "2.2.22",
    "gatsby-source-filesystem": "2.1.39",
    "gatsby-transformer-remark": "2.6.38",
    "gatsby-transformer-sharp": "2.3.6",
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "bundlesize": "0.18.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "node-sass": "4.13.0",
    "prettier": "1.19.1",
    "textlint": "11.5.0"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages-!(photos)-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "30 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  }
}

{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "0.4.1",
    "@fortawesome/fontawesome-svg-core": "1.2.17",
    "@fortawesome/free-brands-svg-icons": "5.8.1",
    "@fortawesome/free-solid-svg-icons": "5.8.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "bootstrap": "4.3.1",
    "font-awesome": "4.7.0",
    "gatsby": "2.4.0",
    "gatsby-image": "2.0.41",
    "gatsby-link": "2.1.0",
    "gatsby-plugin-google-analytics": "2.0.19",
    "gatsby-plugin-manifest": "2.1.0",
    "gatsby-plugin-netlify": "2.0.16",
    "gatsby-plugin-offline": "2.1.0",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sass": "2.0.11",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.0.35",
    "gatsby-plugin-sitemap": "2.1.0",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-transformer-remark": "2.3.12",
    "gatsby-transformer-sharp": "2.1.18",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "bundlesize": "0.17.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.2.0",
    "lint-staged": "8.1.5",
    "node-sass": "4.12.0",
    "prettier": "1.17.0",
    "textlint": "11.2.5"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "npm run lint:textfix",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./public/component---src-pages*.js",
      "maxSize": "10 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "precommit": "lint-staged",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  }
}

{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "1.2.0",
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-brands-svg-icons": "5.10.2",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "bootstrap": "4.3.1",
    "font-awesome": "4.7.0",
    "gatsby": "2.15.14",
    "gatsby-image": "2.2.18",
    "gatsby-link": "2.2.13",
    "gatsby-plugin-google-analytics": "2.1.16",
    "gatsby-plugin-manifest": "2.2.16",
    "gatsby-plugin-netlify": "2.1.12",
    "gatsby-plugin-offline": "3.0.6",
    "gatsby-plugin-react-helmet": "3.1.7",
    "gatsby-plugin-sass": "2.1.14",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.2.21",
    "gatsby-plugin-sitemap": "2.2.12",
    "gatsby-source-filesystem": "2.1.22",
    "gatsby-transformer-remark": "2.6.22",
    "gatsby-transformer-sharp": "2.2.14",
    "lodash": "4.17.15",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "bundlesize": "0.18.0",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "textlint": "11.3.1"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages*.js",
      "maxSize": "15 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  }
}

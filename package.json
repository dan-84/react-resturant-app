{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.0-beta.1",
    "gatsby-image": "^2.0.0-beta.1",
    "gatsby-link": "^2.0.0-beta.1",
    "gatsby-plugin-catch-links": "^2.0.2-beta.1",
    "gatsby-plugin-feed": "^2.0.0-beta.1",
    "gatsby-plugin-google-analytics": "^2.0.0-beta.1",
    "gatsby-plugin-manifest": "^2.0.2-beta.1",
    "gatsby-plugin-netlify": "^2.0.0-beta.1",
    "gatsby-plugin-nprogress": "^2.0.0-beta.1",
    "gatsby-plugin-offline": "^2.0.0-beta.1",
    "gatsby-plugin-preact": "^2.0.0-beta.3",
    "gatsby-plugin-react-helmet": "^3.0.0-beta.1",
    "gatsby-plugin-react-next": "^2.0.1-13",
    "gatsby-plugin-sass": "2.0.7",
    "gatsby-plugin-sharp": "2.0.7",
    "gatsby-plugin-sitemap": "^2.0.0-beta.1",
    "gatsby-plugin-twitter": "^2.0.0-beta.1",
    "gatsby-remark-copy-linked-files": "^2.0.0-beta.1",
    "gatsby-remark-images": "^3.0.0",
    "gatsby-remark-prismjs": "^3.0.0-beta.1",
    "gatsby-remark-responsive-iframe": "^2.0.0-beta.1",
    "gatsby-remark-smartypants": "^2.0.0-beta.1",
    "gatsby-source-filesystem": "^2.0.1-beta.1",
    "gatsby-transformer-remark": "^2.1.1-beta.1",
    "gatsby-transformer-sharp": "^2.1.1-beta.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-brands-svg-icons": "5.6.3",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@fortawesome/react-fontawesome": "0.1.3",
    "animate.css": "3.7.0",
    "babel-eslint": "10.0.1",
    "bootstrap": "4.2.1",
    "bundlesize": "^0.17.0",
    "emergence.js": "1.1.2",
    "eslint": "5.11.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.0",
    "font-awesome": "4.7.0",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "lodash": "4.17.11",
    "modern-normalize": "0.5.0",
    "node-sass": "4.11.0",
    "prettier": "1.15.3",
    "prismjs": "1.15.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0",
    "react-image-lightbox": "5.1.0",
    "textlint": "11.0.2",
    "textlint-rule-preset-ja-spacing": "2.0.1",
    "textlint-rule-preset-japanese": "4.0.0"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "npm run lint:textfix",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./public/component---src-pages*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./public/component---src-templates*.js",
      "maxSize": "10 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "precommit": "lint-staged",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  }
}

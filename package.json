{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "2.0.1",
    "bootstrap": "4.4.1",
    "gatsby": "2.19.15",
    "gatsby-image": "2.2.40",
    "gatsby-link": "2.2.29",
    "gatsby-plugin-google-analytics": "2.1.35",
    "gatsby-plugin-manifest": "2.2.41",
    "gatsby-plugin-netlify": "2.1.32",
    "gatsby-plugin-offline": "3.0.34",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sass": "2.1.28",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.4.5",
    "gatsby-plugin-sitemap": "2.2.27",
    "gatsby-source-filesystem": "2.1.48",
    "gatsby-transformer-remark": "2.6.50",
    "gatsby-transformer-sharp": "2.3.14",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-icons": "3.9.0"
  },
  "devDependencies": {
    "bundlesize": "0.18.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "node-sass": "4.13.1",
    "prettier": "1.19.1",
    "textlint": "11.6.2"
  },
  "resolutions": {
    "**/svgo/js-yaml": "^3.13.1",
    "lodash.template": "^4.5.0",
    "set-value": "^2.0.1"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages-!(photos)-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "30 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  }
}

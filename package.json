{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "0.3.1",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-brands-svg-icons": "5.6.3",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@fortawesome/react-fontawesome": "0.1.3",
    "bootstrap": "4.2.1",
    "font-awesome": "4.7.0",
    "gatsby": "2.0.85",
    "gatsby-image": "2.0.25",
    "gatsby-link": "2.0.7",
    "gatsby-plugin-google-analytics": "2.0.8",
    "gatsby-plugin-manifest": "2.0.13",
    "gatsby-plugin-netlify": "2.0.6",
    "gatsby-plugin-offline": "2.0.20",
    "gatsby-plugin-react-helmet": "3.0.5",
    "gatsby-plugin-sass": "2.0.7",
    "gatsby-plugin-sharp": "2.0.17",
    "gatsby-plugin-sitemap": "2.0.4",
    "gatsby-source-filesystem": "2.0.12",
    "gatsby-transformer-remark": "2.1.19",
    "gatsby-transformer-sharp": "2.1.10",
    "lodash": "4.17.11",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0"
  },
  "devDependencies": {
    "bundlesize": "0.17.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "node-sass": "4.11.0",
    "prettier": "1.15.3",
    "textlint": "11.2.1"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.md": [
      "npm run lint:textfix",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./public/component---src-pages*.js",
      "maxSize": "10 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "precommit": "lint-staged",
    "test": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  }
}

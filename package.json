{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "3.2.1",
    "bootstrap": "4.5.0",
    "gatsby": "2.21.22",
    "gatsby-image": "2.4.3",
    "gatsby-link": "2.4.2",
    "gatsby-plugin-google-analytics": "2.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-netlify": "2.3.2",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sass": "2.3.1",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-plugin-sitemap": "2.4.2",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "gatsby-transformer-sharp": "2.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-icons": "3.10.0"
  },
  "devDependencies": {
    "bundlesize": "0.18.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "textlint": "11.6.3"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "bundlesize": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages-!(photos)-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "35 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlesize": "yarn run build",
    "bundlesize": "bundlesize",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:text": "textlint src/pages/**/index.md",
    "lint:textfix": "textlint --fix src/pages/**/index.md",
    "test": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  }
}

{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "4.0.2",
    "@browniebroke/react-ui-components": "1.6.0",
    "bootstrap": "4.5.3",
    "gatsby": "2.27.5",
    "gatsby-image": "2.6.0",
    "gatsby-link": "2.6.1",
    "gatsby-plugin-google-analytics": "2.6.0",
    "gatsby-plugin-manifest": "2.7.0",
    "gatsby-plugin-netlify": "2.6.0",
    "gatsby-plugin-offline": "3.5.0",
    "gatsby-plugin-react-helmet": "3.5.0",
    "gatsby-plugin-robots-txt": "1.5.3",
    "gatsby-plugin-sass": "2.6.0",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.9.1",
    "gatsby-plugin-sitemap": "2.7.0",
    "gatsby-source-filesystem": "2.6.1",
    "gatsby-transformer-remark": "2.11.1",
    "gatsby-transformer-sharp": "2.7.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "4.1.0"
  },
  "devDependencies": {
    "bundlewatch": "0.3.1",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "node-sass": "4.14.1",
    "prettier": "2.2.1",
    "textlint": "11.7.6"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages-!(photos)-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "35 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlewatch": "yarn run build",
    "bundlewatch": "bundlewatch",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint:eslint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:prettier": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  }
}

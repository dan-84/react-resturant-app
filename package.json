{
  "name": "chez-nicole-web",
  "description": "Website for Chez Nicole",
  "version": "2.0.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "dependencies": {
    "@browniebroke/gatsby-image-gallery": "3.5.1",
    "bootstrap": "4.5.0",
    "gatsby": "2.24.3",
    "gatsby-image": "2.4.13",
    "gatsby-link": "2.4.13",
    "gatsby-plugin-google-analytics": "2.3.11",
    "gatsby-plugin-manifest": "2.4.18",
    "gatsby-plugin-netlify": "2.3.11",
    "gatsby-plugin-offline": "3.2.18",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sass": "2.3.12",
    "gatsby-plugin-sentry": "1.0.1",
    "gatsby-plugin-sharp": "2.6.19",
    "gatsby-plugin-sitemap": "2.4.11",
    "gatsby-source-filesystem": "2.3.19",
    "gatsby-transformer-remark": "2.8.25",
    "gatsby-transformer-sharp": "2.5.11",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.10.0"
  },
  "devDependencies": {
    "bundlewatch": "0.2.7",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "textlint": "11.7.6"
  },
  "keywords": [
    "gatsby",
    "gatstrap",
    "starter"
  ],
  "license": "MIT",
  "bundlewatch": [
    {
      "path": "./public/app-*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./public/component---src-pages-!(photos)-*.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/component---src-pages-photos-*.js",
      "maxSize": "35 kB"
    }
  ],
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "prebundlewatch": "yarn run build",
    "bundlewatch": "bundlewatch",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.+(js|json|md)\"",
    "lint:eslint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:prettier": "prettier --check \"**/*.+(js|jsx|json|css|scss)\""
  }
}
